{
  "name": "@jonallured/jay",
  "description": "Various cli tricks and stunts.",
  "version": "0.0.1",
  "author": "Jon Allured @jonallured",
  "bin": {
    "jay": "./bin/run"
  },
  "bugs": "https://github.com/jonallured/jay/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.0",
    "moment": "^2.27.0",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@types/jest": "^26.0.8",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "globby": "^11.0.1",
    "jest": "^26.2.2",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.4",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/jonallured/jay",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx|js|jsx)$": "ts-jest"
    },
    "testRegex": ".test\\.(ts|js)",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "jay",
    "hooks": {
      "init": "src/hooks/init.ts"
    },
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "keys": {
        "description": "Manage authorized keys."
      }
    }
  },
  "repository": "jonallured/jay",
  "prettier": {
    "semi": false
  },
  "scripts": {
    "build": "tsc && oclif-dev manifest",
    "lint": "eslint 'src/**/*.ts' 'test/**/*.ts'",
    "prettier": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "prettier-check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc --pretty --noEmit"
  },
  "types": "lib/index.d.ts"
}
