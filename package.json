{
  "name": "@jonallured/jay",
  "description": "Various cli tricks and stunts.",
  "version": "0.0.3",
  "author": "Jon Allured @jonallured",
  "bin": {
    "jay": "./bin/run"
  },
  "bugs": "https://github.com/jonallured/jay/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.0",
    "@octokit/rest": "^18.0.6",
    "node-fetch": "^2.6.1",
    "tslib": "^2.0.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@octokit/types": "^5.5.0",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "@types/node-fetch": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "directory-tree": "^2.2.4",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "globby": "^11.0.1",
    "jest": "^26.4.2",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/jonallured/jay",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts",
      "tsx"
    ],
    "testEnvironment": "node",
    "testRegex": ".test\\.(ts|js)$",
    "transform": {
      "^.+\\.(ts|tsx|js|jsx)$": "ts-jest"
    }
  },
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "jay",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "keys": {
        "description": "Manage authorized keys."
      },
      "sharpen": {
        "description": "Sharpen your tools."
      }
    }
  },
  "repository": "jonallured/jay",
  "prettier": {
    "semi": false
  },
  "scripts": {
    "build": "tsc && oclif-dev manifest",
    "lint": "eslint 'src/**/*.ts' 'test/**/*.ts'",
    "prettier": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "prettier-check": "prettier --check 'src/**/*.ts' 'test/**/*.ts'",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "tsc --pretty --noEmit"
  },
  "types": "lib/index.d.ts"
}
